<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_main">

    <fragment
        android:id="@+id/nav_Restaurantes"
        android:name="com.example.movilesproyectofinal.ui.fragment.RestaurantesFragment"
        android:label="Restaurantes"
        tools:layout="@layout/fragment_restaurantes_list" />

    <fragment
        android:id="@+id/nav_RestauranteDescripcion"
        android:name="com.example.movilesproyectofinal.ui.fragment.RestauranteDescripcionFragment"
